(:name all-the-icons
       :website "https://github.com/domtronn/all-the-icons.el"
       :description "A utility package to collect various Icon Fonts for emacs"
       :type github
       :pkgname "domtronn/all-the-icons.el"
       :build `((,el-get-emacs "--quick" "--batch" "--directory" "." "--load" "all-the-icons" "--eval" "(all-the-icons-install-fonts 'yes)")))
